<div class="modal fade" id="editanggota" tabindex="-1" role="dialog" aria-labelledby="editanggotaLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="editanggotaLabel">Tambah anggota</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
      </div>
      <form action="anggota/edit_anggota?_method=PUT" method="post">   
          <div class="form-group col-md-12">
              <label for="">NIM</label> 
              <input type="number" name="nim" class="form-control nim" required>     
          </div>
          <div class="form-group col-md-12">
              <label for="">Nama</label> 
              <input type="text" name="nama" class="form-control nama" required>     
          </div>
          <div class="form-group col-md-12">
              <label for="">Gender</label>  
              <input type="text" name="Jns_kelamin" class="form-control Jns_kelamin" required>     
          </div>      
          <div class="form-group col-md-12">
              <label for="">Alamat</label> 
              <input type="text" name="alamat" class="form-control alamat" required>     
          </div>      
          <div class="form-group col-md-12">
              <label for="">Telephon</label> 
              <input type="number" name="telp" class="form-control telp" required>     
          </div>              
          <div class="form-group col-md-12">
            <label for="">Email</label> 
            <input type="email" name="email" class="form-control email" required>     
        </div>      
        <div class="form-group col-md-12">
          <label for="">Tangggal lahir</label> 
          <input type="date" name="ttl" class="form-control ttl" required>     
        </div>      
               <div class="modal-footer">
                <input type="hidden" name="id" class="id">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="submit" class="btn btn-primary">Edit anggota</button>
              </div>
      </form>
    </div>
  </div>
</div>